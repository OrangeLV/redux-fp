    "watch": "npm run clean:build && rollup -c -w",
    "pretest": "npm run clean:test && rollup -c --environment TESTING",
    "test": "flow && mocha",
    "prepublish": "npm test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvikmanis/redux-curried-reducers.git"
