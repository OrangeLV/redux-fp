  "description": "Sample project for packages built with rollup.",
  "main": "dist/redux-curried-reducers.js",
  "module": "dist/redux-curried-reducers.mjs",
  "jsnext:main": "dist/redux-curried-reducers.mjs",
  "scripts": {
    "clean:test": "rm -rf tmp coverage",
    "clean:build": "rm -rf dist",
